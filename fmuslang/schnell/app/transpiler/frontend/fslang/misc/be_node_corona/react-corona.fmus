    let data = response.result
      .map((record, index) => {
        record.key = index;
        return recordTransformer(record);
      });

    set_table_data(data);
  };

  const wrapAsync = () => {
    PubSub.subscribe('<<Create:Record>>', update_table_data);
    PubSub.subscribe('<<Update:Record>>', update_table_data);
    PubSub.subscribe('<<Delete:Record>>', update_table_data);
  }

