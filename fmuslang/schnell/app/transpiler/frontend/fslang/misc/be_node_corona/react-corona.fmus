	useEffect(function() {
		setToolbar(<Toolbar />)
	}, []);

	return <>
		<TabPage 
			tabpos={lokasiTab}
			tabs_data={
				[
					{
						title: 'Create',
						icon: <DesktopOutlined />,
						component: <FormWrapper />
					},
