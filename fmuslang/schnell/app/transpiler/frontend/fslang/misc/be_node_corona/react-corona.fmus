export const create_series_item = (item_name, item_data, markpoint_data=[], chart_type='line', smooth=false, area=false) => {

    let conditional = {
        name: item_name,
        type: chart_type,
        data: item_data,
        markPoint: {
            data: markpoint_data
        },
        markLine: {
            data : [
                {type : 'average', name: 'Average'}
            ]
        }
    }
    if (area) conditional.areaStyle = {};
    if (smooth) conditional.smooth = true;
    return conditional;
}

