        ) : null,
      };

    } else {

      let noFilter = {
        // kolom biasa, dataIndex sama dg nama kolom
        title: column,
        fixed: index == 1 ? 'left' : null,
        width: index == 1 ? JsonData.widths.id : JsonData.widths.normal,
