  // console.log(`table sorting: ${column}, pertama ${a[column]}, kedua ${b[column]}.`)
  // return b[column] > a[column]
  // return b[column].localeCompare(a[column])
  // return a[column] - b[column]
  // return a[column] > b[column]
  if (b[column] === null || b[column] === '') // descend
