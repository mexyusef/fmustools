      ? 0
      : amplitude * Math.sin((index * 2 * Math.PI) / length - 1) +
        // _random(-amplitude, amplitude)
        _.random(-amplitude, amplitude);
    return Math.max(0, Math.round(lineEquation(index) - diff));
  }

  return Array(length)
    .fill(null)
    .map((item, index) => getPointData(index))
}

