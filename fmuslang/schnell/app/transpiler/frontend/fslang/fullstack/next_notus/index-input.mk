--% /dahsyat/javascript/next-notus/components/Sidebar/SubMenu.js
import Link from "next/link";
import { useRouter } from "next/router";

const fontawesome_icons = [
  'fas fa-fingerprint',
  'fas fa-clipboard-list',
  'fas fa-newspaper',
  'fas fa-tv',
  'fas fa-tools',
  'fas fa-table',
  'fas fa-map-marked',
  'fas fa-user-circle',
];

// items = [{label,link}]
const SubMenu = ({
  header, items, divider=true,
}) => {

  const router = useRouter();

	return (<>
    {divider?<hr className="my-4 md:min-w-full" />:null}
    <h6 className="md:min-w-full text-blueGray-500 text-xs uppercase font-bold block pt-1 pb-4 no-underline">
      {header}
    </h6>

    <ul className="md:flex-col md:min-w-full flex flex-col list-none md:mb-4">
      {items.map((record,index) => {
        // let list_index = Math.floor(Math.random() * (fontawesome_icons.length - 0 + 1));
        // let item_icon = fontawesome_icons[list_index];
        let item_icon = record.icon ? record.icon : fontawesome_icons[0];
        let selected = router.pathname.indexOf(record.link) !== -1;
        let menu_item_color = selected ? "text-lightBlue-500 hover:text-lightBlue-600" : "text-blueGray-700 hover:text-blueGray-500 opacity-75";
        return (<li className="items-center" key={index}>
          <Link href={record.link}>
            <a
              href="#"
              className="text-blueGray-700 hover:text-blueGray-500 text-xs uppercase py-3 font-bold block"
            >
              <i className={`${item_icon} mr-2 text-sm ${menu_item_color}`}></i>{" "}
              {record.label}
            </a>
          </Link>
        </li>);
      })}
    </ul>

  </>);
};

export default SubMenu;

