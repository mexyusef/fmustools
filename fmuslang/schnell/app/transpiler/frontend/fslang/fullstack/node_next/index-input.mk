    } else {
      UserAPI.follow(username)
    }
    setFollowing(!following)
  }
  return (
    <button
      className={`btn btn-sm action-btn ${
        following ? 'btn-secondary' : 'btn-outline-secondary'
      }`}
      onClick={handleClick}
    >
      <i className="ion-plus-round" /> &nbsp;{' '}
      {following ? 'Unfollow' : 'Follow'} {username}
    </button>
  )
}

